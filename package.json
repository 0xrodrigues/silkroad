{
  "name": "silkroad",
  "version": "1.0.0",
  "description": "Silk Road - Integrated Node.js Marketplace with Backend and Frontend",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon src/server/index.js",
    "dev:client": "vite",
    "build": "vite build",
    "build:server": "node src/server/index.js",
    "start": "NODE_ENV=production node src/server/index.js",
    "db:migrate": "node src/server/database/migrate.js"
  },
  "keywords": ["marketplace", "ecommerce", "node", "express", "react", "postgresql"],
  "author": "VoidX Company",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
